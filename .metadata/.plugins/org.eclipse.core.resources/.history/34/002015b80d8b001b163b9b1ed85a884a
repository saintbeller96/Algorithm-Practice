package P1953최단경로;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.PriorityQueue;
import java.util.StringTokenizer;

public class Main_DIJK2 {
	//정점 클래스
	static class Vertex{
		//n은 정점 번호, cost는 정점까지 비용 저장
		int n, cost;
		public Vertex(int n, int cost) {
			this.n = n;
			this.cost = cost;
		}
	}
	static class Edge{
		//u: 시작, v: 도착, w: 가중치
		int u, v, w;
		public Edge(int u, int v, int w) {
			this.u = u;
			this.v = v;
			this.w = w;
		}
	}
	static int V, E;
	static ArrayList<Edge>[] list;//인접 리스트
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer stk = new StringTokenizer(br.readLine());
		V = Integer.parseInt(stk.nextToken());
		E = Integer.parseInt(stk.nextToken());
		int s = Integer.parseInt(br.readLine());

		//인접 리스트 생성
		list = new ArrayList[V+1];
		for(int i = 0; i<=V; i++) {
			list[i] = new ArrayList<Edge>();
		}
		for(int i = 0; i<E; i++) {
			stk = new StringTokenizer(br.readLine());
			int u = Integer.parseInt(stk.nextToken());
			int v = Integer.parseInt(stk.nextToken());
			int w = Integer.parseInt(stk.nextToken());
			//인접 리스트에 간선 정보 저장
			list[u].add(new Edge(u, v, w));
		}
		dijk(s);
	}
	
	static int dijk(int start) {
		//cost 값을 기준으로하는 우선순위큐
		PriorityQueue<Vertex> que = new PriorityQueue<Vertex>((a, b)->Integer.compare(a.cost, b.cost));
		
		//dist: 정점에 기록되는 최단거리 기록
		int[] dist = new int[V+1];
		for(int i = 1; i<=V; i++) {
			if(i == start) {
				dist[i] = 0;
			}
			else {
				dist[i] = Integer.MAX_VALUE;
			}
		}
		que.offer(new Vertex(start, 0));

		while(!que.isEmpty()) {
			Vertex u = que.poll();
			//현재 u에 기록된 최단거리가 더 적으면 갱신 시작
			if(dist[u.n] >= u.cost) {
				for(Edge e : list[u.n]) {
					int z = e.v;//도착 정점
					int cost = e.w;//사이에 있는 간선의 가중치
					
					//현재 기록된 최단거리보다 이번에 계산한 최단거리가 더 작은 경우
					if(dist[z] > u.cost + cost) {
						//최단거리 갱신 후 큐에 넣음
						dist[z] = u.cost + cost;
						que.offer(new Vertex(z, u.cost + cost));
					}
				}
			}
		}
		//출력
		for(int i = 1; i<=V; i++) {
			if(i == start) {
				System.out.println(0);
			}
			else if(dist[i] == Integer.MAX_VALUE) {
				System.out.println("INF");
			}else {
				System.out.println(dist[i]);
			}
		}
		return 0;
	}
}
